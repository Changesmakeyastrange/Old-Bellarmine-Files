public class Dicea
{
	public static void main ( String args[] )
	{
	String in;
	
	double n,
		   f;

	int[] numbers = new int [6];
	
	for (int i = 0; i < 10000; i++)
	{

		f = (int)(1 + Math.random() * 6);

		if (f == 1)
		{
			numbers[1]++;
		}
		if (f == 2)
		{
			numbers[2]++;
		}
		if (f == 3)
